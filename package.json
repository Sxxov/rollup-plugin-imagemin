{
  "name": "rollup-plugin-imagemin",
  "version": "0.2.0",
  "description": "Imagemin meets Rollup!",
  "module": "dist/index.es.mjs",
  "jsnext:main": "dist/index.es.mjs",
  "main": "dist/index.cjs.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npm run clean && npx rollup -c rollup.config.js",
    "lint": "npx eslint src/index.mjs test/index.mjs",
    "test": "npx mocha --require @babel/register ./test/index.mjs",
    "prepublish": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/malchata/rollup-plugin-imagemin.git"
  },
  "keywords": [
    "image",
    "optimization",
    "rollup",
    "performance",
    "webperf"
  ],
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/rollup-plugin-imagemin/issues"
  },
  "homepage": "https://github.com/malchata/rollup-plugin-imagemin#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "imagemin": "^6.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "mkpath": "^1.0.0",
    "rollup-pluginutils": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "eslint": "^5.10.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3"
  }
}
